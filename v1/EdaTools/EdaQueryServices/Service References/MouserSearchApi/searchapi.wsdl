        </s:sequence>
      </s:complexType>
      <s:simpleType name="MCalculatorMethod">
        <s:restriction base="s:string">
          <s:enumeration value="None" />
          <s:enumeration value="ResistanceCode" />
          <s:enumeration value="ResistanceColorCode1" />
          <s:enumeration value="ResistanceColorCode2" />
          <s:enumeration value="CapacitorCode" />
          <s:enumeration value="Inductance" />
