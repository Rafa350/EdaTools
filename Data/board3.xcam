<?xml version="1.0" encoding="utf-8"?>
<cam version="100">
    <panel>
        <board fileName="board3.xml" position="0, 0" orientation="0"/>
        <board fileName="board3.xml" position="100, 0" orientation="0"/>
        <board fileName="board3.xml" position="0, 100" orientation="0"/>
        <board fileName="board3.xml" position="100, 100" orientation="0"/>
    </panel>
    
    <targets>
        <!-- Capa coure L1 top -->
        <target target="$(targetFolder)$(fName)_Copper$L1.gbr" type="gerber-image">
            <layers>
                <layer name="Top"/>
            </layers>
        </target>
     
        <!-- Capa coure L2 bottom -->
        <target target="$(targetFolder)$(fName)_Copper$L2.gbr" type="gerber-image">
            <layers>
                <layer name="Bottom"/>
            </layers>
        </target>
        
        <!-- Capa silk top -->
        <target target="$(targetFolder)$(fName)_Legend$Top.gbr" type="gerber-image">
            <layers>
                <layer name="TopPlace"/>
                <layer name="TopNames"/>
            </layers>
        </target>
        
        <!-- Capa silk bottom -->
        <target target="$(targetFolder)$(fName)_Legend$Bottom.gbr" type="gerber-image">
            <layers>
                <layer name="BottomPlace"/>
                <layer name="BottomNames"/>
            </layers>
        </target>

        <!-- Perfil -->
        <target target="$(targetFolder)$(fName)_Profile$NP.gbr" type="gerber-image">
            <layers>
                <layer name="Profile"/>
            </layers>
        </target>

        <!-- Forats no platejats capes 1-2 -->
        <target target="$(targetFolder)$(fname)_NonPlated$1$2$NPTH$Drill.gbr" type="gerber-drill">
            <layers>
                <layer name="Holes"/>
            </layers>
            <options>
                <option name="drillType" value="NonPlated"/>
                <option name="topLayer" value="1"/>
                <option name="bottomLayer" value="2"/>
            </options>
        </target>

        <!-- Forats platejats capes 1-2 -->
        <target target="$(targetFolder)$(fName)_Plated$1$2$PTH$Drill.gbr" type="gerber-drill">
            <layers>
                <layer name="Drills"/>
            </layers>
            <options>
                <option name="drillType" value="Plated"/>
                <option name="topLayer" value="1"/>
                <option name="bottomLayer" value="2"/>
            </options>
        </target>

    </targets>
</cam>
